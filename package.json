{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "starter node in typescript",
  "main": "index.js",
  "scripts": {
    "serve": "node dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run watch-ts\" \"nodemon dist/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isebmo/node-ts-starter.git"
  },
  "keywords": [
    "node"
  ],
  "author": "isebmo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isebmo/node-ts-starter/issues"
  },
  "homepage": "https://github.com/isebmo/node-ts-starter#readme",
  "dependencies": {
    "express": "^4.15.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.29",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.4"
  }
}